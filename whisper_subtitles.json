[
  {
    "index": 1,
    "text": "أعوذ بالله من الشيطان الرجيم",
    "start": 1.0,
    "end": 7.82,
    "duration": 6.82,
    "source": "whisper"
  },
  {
    "index": 2,
    "text": "بسم الله الرحمن الرحيم",
    "start": 7.82,
    "end": 13.56,
    "duration": 5.74,
    "source": "whisper"
  },
  {
    "index": 3,
    "text": "إذا السماء انفطرت",
    "start": 13.56,
    "end": 22.2,
    "duration": 8.64,
    "source": "whisper"
  },
  {
    "index": 4,
    "text": "وإذا الكواكب انتثرت",
    "start": 22.2,
    "end": 28.06,
    "duration": 5.86,
    "source": "whisper"
  },
  {
    "index": 5,
    "text": "وإذا البحار فجرت",
    "start": 28.06,
    "end": 32.36,
    "duration": 4.3,
    "source": "whisper"
  },
  {
    "index": 6,
    "text": "وإذا القبور بعثرت",
    "start": 32.36,
    "end": 36.6,
    "duration": 4.24,
    "source": "whisper"
  },
  {
    "index": 7,
    "text": "علمت نفس ما قدمت وأخرت",
    "start": 36.6,
    "end": 43.96,
    "duration": 7.36,
    "source": "whisper"
  },
  {
    "index": 8,
    "text": "يا أيها الإنسان ما غرك بربك الكريم",
    "start": 43.96,
    "end": 55.1,
    "duration": 11.14,
    "source": "whisper"
  },
  {
    "index": 9,
    "text": "الذي خلقك فسواك فعدلك في أي صورة ما شاء ركبك",
    "start": 55.1,
    "end": 69.26,
    "duration": 14.16,
    "source": "whisper"
  },
  {
    "index": 10,
    "text": "كلا بل تكذبون بالدين",
    "start": 69.26,
    "end": 75.42,
    "duration": 6.16,
    "source": "whisper"
  },
  {
    "index": 11,
    "text": "وإن عليكم لحافذين",
    "start": 75.42,
    "end": 82.36,
    "duration": 6.94,
    "source": "whisper"
  },
  {
    "index": 12,
    "text": "كراما كاتبين",
    "start": 82.36,
    "end": 88.72,
    "duration": 6.36,
    "source": "whisper"
  },
  {
    "index": 13,
    "text": "يعلمون ما تفعلون",
    "start": 88.72,
    "end": 94.48,
    "duration": 5.76,
    "source": "whisper"
  },
  {
    "index": 14,
    "text": "إن الأبرار لفي نعيم",
    "start": 94.48,
    "end": 101.96,
    "duration": 7.48,
    "source": "whisper"
  },
  {
    "index": 15,
    "text": "وإن الفجار لفي جحيم",
    "start": 101.96,
    "end": 110.1,
    "duration": 8.14,
    "source": "whisper"
  },
  {
    "index": 16,
    "text": "يصلونها يوم الدين وما هم عنها بغائبين",
    "start": 110.1,
    "end": 120.28,
    "duration": 10.18,
    "source": "whisper"
  },
  {
    "index": 17,
    "text": "وما أدراك ما يوم الدين",
    "start": 120.28,
    "end": 127.88,
    "duration": 7.6,
    "source": "whisper"
  },
  {
    "index": 18,
    "text": "ثم ما أدراك ما يوم الدين",
    "start": 127.88,
    "end": 137.58,
    "duration": 9.7,
    "source": "whisper"
  },
  {
    "index": 19,
    "text": "يوم لا تملك نفس لنفس شيئا",
    "start": 137.58,
    "end": 145.66,
    "duration": 8.08,
    "source": "whisper"
  },
  {
    "index": 20,
    "text": "والأمر يومئذ لله",
    "start": 145.66,
    "end": 151.26,
    "duration": 5.6,
    "source": "whisper"
  }
]